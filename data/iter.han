        proc mergesort_singleton(caller) {
            let (q) = this.super.super.once(@foo);

            let (a) = this.super.super.builtin.compose(q, this.super.super.mergesort);
            let (@resp, a, @yield, @foo) = a(@next);
            let (@end, @eos) = a(@next);

            let () = caller(@pass);
