        proc alternate(caller) {
            let (s) = this.super.super.alternate();
            let (@req, s, @next) = s(@get);
            let (@req, s, @next) = s(@yield, 'f');
            let (@req, s, @next) = s(@yield, 'o');
            let (@req, s, @next) = s(@yield, 'o');
            let (@req, s, @next) = s(@yield, 'b');
            let (@req, s, @next) = s(@yield, 'a');
