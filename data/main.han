mod iter {
    proc symbol_chars_else(caller, symbol, offset) {
        let (char) = this.symbol_char_at(*offset, *symbol);
        let (caller) = caller(@yield, char);
        let (next_offset) = this.add(offset, 1);
        become this.symbol_chars_inner(next_offset, symbol, caller)
    }


    proc symbol_chars_inner() {
        R {
            [@next, caller, offset, symbol]
            *symbol 
            {
                [caller, offset, symbol, len, @ok, ^]
                *offset
                *len
                {
                    [caller, offset, symbol, len, are_eq, @ok, ^]
                    are_eq
                    {
                        {}
                    }
                    {
                        [caller, offset, symbol, len]
                        *symbol *offset #symbol_char_at
                        @yield
                        {
                            {}
                        }
                        caller
                        @exec
                    }
                    #if
                    @exec
                
                }
                this.super.builtin.eq
                @exec
            }
            this.super.builtin.symbol_len
            @exec
        }
    }

    
    proc symbol_chars_inner2(caller, symbol, offset) {
        let (_, @ok, len) = this.super.builtin.symbol_len(*symbol);
        let (_, @ok, are_eq) = this.super.builtin.eq(*offset, *len);
        R {
            [are_eq, caller]
            {}
        }
