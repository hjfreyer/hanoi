
mod builtin {
    proc symbol_len(caller, symbol) {
        R{ 
            [symbol, caller]
            symbol #symbol_len @ok this.symbol_len caller @exec 
        }
    }

    proc eq(caller, a, b) {
        R{ 
            [b, a, caller]
            a b #eq @ok this.eq caller @exec 
        }
    }

